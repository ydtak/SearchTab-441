{
  "manifest_version": 2,

  "name": "SearchTab",
  "description": "DAAB Ftw.",
  "version": "1.0",

  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "SearchTab"
  },
  "permissions": [
    "tabs",
    "history"
  ]
}